{
  "name": "@vingle/cryptobadge-sdk",
  "version": "1.0.0",
  "description": "",
  "main": "./dst/index.js",
  "typings": "./dst/index.d.ts",
  "scripts": {
    "pretest": "npm run build -- -p ./tsconfig.test.json",
    "test": "mocha -t 30000 dst/**/__test__/**/*_spec.js",
    "build": "rm -Rf dst && tsc -d",
    "lint": "tslint -p tsconfig.test.json",
    "prepublish": "npm run build",
    "generate": "apollo codegen:generate --target typescript --outputFlat --globalTypesFile=./src/__generated__/global.ts ./src/queries/types.ts && ts-node ./scripts/build-query.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balmbees/cryptobadge-sdk-js.git"
  },
  "files": [
    "dst/**/*.js",
    "dst/**/*.d.ts",
    "dst/**/*.json"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "4.2.3",
    "@types/common-tags": "1.8.0",
    "@types/graphql": "14.2.3",
    "@types/mocha": "5.2.7",
    "@types/node-fetch": "2.5.2",
    "@vingle/commitlint-preset": "1.0.3",
    "@vingle/semantic-release-config": "1.0.3",
    "@vingle/tslint-preset": "1.0.0",
    "apollo": "2.20.0",
    "chai": "4.2.0",
    "common-tags": "1.8.0",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "husky": "3.0.9",
    "mocha": "6.2.2",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-http": "^1.5.15",
    "node-fetch": "^2.6.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@vingle/commitlint-preset"
    ]
  }
}
